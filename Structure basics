#include <stdio.h>
typedef struct 
{
    char name[20];
    int id;
}emp;

int main()
{
   // typedef struct employee emp;
    int i;
    emp e[3];
    //={{"sanjay",2000},{"aakash",2001},{"pathikrit",2002}};
    for(i=0;i<3;i++)
    {
        scanf("%s%d",&e[i].name,&e[i].id);
    }
     for(i=0;i<3;i++)
    {
        printf("%s\t%d\n",e[i].name,e[i].id);
    }

    return 0;
}
